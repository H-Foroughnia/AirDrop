            "type": "integer",
            "format": "int64"
          },
          "creationDate": {
            "type": "string",
            "format": "date-time"
          },
          "isDelete": {
            "type": "boolean"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "score": {
            "type": "integer",
            "format": "int64"
          },
          "taskCategoryId": {
            "type": "integer",
            "format": "int64"
          },
          "sampleImage": {
            "type": "string",
            "nullable": true
          },
          "taskImages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AirDropTaskImage"
            },
            "nullable": true
          },
          "taskCategory": {
            "$ref": "#/components/schemas/AirDropTaskCategory"
          }
        },
        "additionalProperties": false
      },
      "AirDropTaskCategory": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "creationDate": {
            "type": "string",
            "format": "date-time"
          },
          "isDelete": {
            "type": "boolean"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "tasks": {
            "type": "array",
