            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AirDropTaskImage": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "creationDate": {
            "type": "string",
            "format": "date-time"
          },
          "isDelete": {
            "type": "boolean"
          },
          "userId": {
            "type": "integer",
            "format": "int64"
          },
          "taskId": {
            "type": "integer",
            "format": "int64"
          },
          "imageName": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "task": {
            "$ref": "#/components/schemas/AirDropTask"
          }
        },
        "additionalProperties": false
      },
      "TelegramAuthRequestDto": {
        "type": "object",
        "properties": {
          "telegramId": {
            "type": "string",
            "nullable": true
