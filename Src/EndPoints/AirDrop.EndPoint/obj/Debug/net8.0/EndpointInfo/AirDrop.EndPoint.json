{
  "openapi": "3.0.1",
  "info": {
    "title": "AirDrop.EndPoint",
    "version": "1.0"
  },
  "paths": {
    "/api/auth/telegram": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TelegramAuthRequestDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TelegramAuthRequestDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TelegramAuthRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/category": {
      "get": {
        "tags": [
          "Category"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AirDropTaskCategory"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AirDropTaskCategory"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AirDropTaskCategory"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/taskimage": {
      "get": {
        "tags": [
          "TaskImage"
        ],
        "operationId": "GetTaskByCategory",
        "parameters": [
          {
            "name": "categoryId",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AirDropTask"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AirDropTask"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AirDropTask"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "TaskImage"
        ],
        "operationId": "DoTask",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "UserId": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "TaskId": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "ImageName": {
                    "type": "string",
                    "format": "binary"
                  },
                  "Status": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "UserId": {
                  "style": "form"
                },
                "TaskId": {
                  "style": "form"
                },
                "ImageName": {
                  "style": "form"
                },
                "Status": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AirDropTask": {
        "type": "object",
